<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.eight.leqia.mapper.CustomerMapperT">
    <insert id="add" parameterType="String">
        insert into Customer(UserName,HeadPortrait)values(#{UserName},#{HeadPortrait})
    </insert>
    <select id="selCustom" resultType="Customer" parameterType="String">
        select * from Customer where UserName=#{UserName} and HeadPortrait=#{HeadPortrait}
    </select>



</mapper>